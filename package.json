{
  "name": "object0",
  "private": true,
  "version": "1.2.0",
  "description": "FOSS desktop S3 bucket manager built with Tauri",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:app": "bun run tauri dev",
    "build": "tsc && vite build",
    "build:app": "bun run tauri build",
    "preview": "vite preview",
    "check": "biome check .",
    "check:all": "bun run check && bun run build && cd src-tauri && cargo check",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "release:sync-version": "node ./scripts/release/sync-version.mjs",
    "updater:manifest": "bun ./scripts/updater/build-static-manifest.mjs",
    "updater:verify": "bun ./scripts/updater/verify-static-manifest.mjs",
    "tauri": "tauri"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.1.0",
    "@tanstack/react-virtual": "^3.13.18",
    "@tauri-apps/api": "^2.10.1",
    "@tauri-apps/plugin-opener": "^2.5.3",
    "framer-motion": "^12.34.0",
    "qrcode.react": "^4.2.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "sonner": "^2.0.7",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@tailwindcss/vite": "^4.1.18",
    "@tauri-apps/cli": "^2.10.0",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "daisyui": "^5.5.18",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "vite": "^7.3.1"
  }
}
